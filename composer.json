{
    "name": "diviky/secure",
    "description": "Laravel-aware PHP code obfuscator for securing your packages without breaking dependencies",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar",
            "email": "sankar@diviky.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "nikic/php-parser": "^5.0",
        "symfony/console": "^7.0",
        "symfony/config": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/finder": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Diviky\\Secure\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diviky\\Secure\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/secure"
    ],
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    }
} 